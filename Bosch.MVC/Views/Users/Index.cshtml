@model List<Entities.Concretes.User>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<a href="@Url.Action("Create")" class="btn btn-success">Add New User</a>
<ul>
    @foreach (var item in Model)
    {
        <li> <a href="@Url.Action("Details", new { id=item.Id})"> @item.Username </a></li>
    }
</ul>